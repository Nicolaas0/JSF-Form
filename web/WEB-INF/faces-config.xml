<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
<!--Configuration for Home page -->
<navigation-rule>
<from-view-id>/index.xhtml</from-view-id>
<navigation-case>
<from-action>#{bean.Action}</from-action>
<from-outcome>student</from-outcome>
<to-view-id>/StudentProfile.xhtml</to-view-id>
</navigation-case>
<navigation-case>
<from-action>#{bean.Action}</from-action>
<from-outcome>error</from-outcome>
<to-view-id>/LoginError.xhtml</to-view-id>
</navigation-case>
</navigation-rule>
<!--Configuration for Success page -->
<navigation-rule>
<from-view-id>/success.xhtml</from-view-id>
